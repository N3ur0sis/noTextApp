# Environment Variables for NoText App
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# JWT Secret (for development - use a strong random string in production)
JWT_SECRET=your_jwt_secret_here

# Supabase Service Role Key (only needed for admin operations)
service_role=your_supabase_service_role_key_here

# Expo Configuration
EXPO_PUBLIC_PROJECT_ID=your_expo_project_id_here